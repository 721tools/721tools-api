{
	"compilerOptions": {
		"module": "commonjs",
		"moduleResolution": "node",
		"pretty": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"sourceMap": true,
		"target": "es2019",
		"outDir": "./dist",
		"baseUrl": "./src"
	},
	"include": [
		"src/**/*.ts"
	],
	"exclude": [
		"node_modules"
	]
}